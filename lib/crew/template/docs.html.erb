  <body>
    <h1><a name="top">Crew - Tasks</a></h1>

    <h2>Sources</h2>
    <% home.sources.each do |source| %>
      <a href="#source_<%=source.hash%>"><%=source%></a>
    <% end %>

    <h2>All tasks</h2>
    <div class="legend" id="all-tasks">
      <% home.each_task do |task| %>
        <a href="#<%=task.name%>"><%=task.name%></a>
      <% end %>
    </div>

    <div id="task-search-area">
      <input type="text" onkeyup="searchTasks(this.value)" placeholder="Search" class="searchbox">
      <div id="task-search-results"></div>
    </div>

    <div id="full-tasks">
    <% home.each_task do |task| %>
      <%= render_task(task) %>
    <% end %>
    </div>

    <script>
      var list = document.getElementsByTagName('code');
      for (var i = 0; i != list.length; i++) {
        var e = list[i];
        hljs.highlightBlock(e);
      }

      function Trie() {
        this.value = [];
        this.dictionary = new Object();
        this.insert = function(key, value) {
          if (key.length == 0) {
            this.value.push(value);
          } else if (key.length > 0) {
            var c = key.charAt(0);
            if (typeof(this.dictionary[c]) == 'undefined') {
              this.dictionary[c] = new Trie();
            }
            this.dictionary[c].insert(key.slice(1), value);
          }
        }
        this.searchTree = function(key) {
          if (key.length == 0) {
            return this;
          }
          if (!this.dictionary.hasOwnProperty(key.charAt(0))) {
            return new Trie();
          }
          return this.dictionary[key.charAt(0)].searchTree(key.slice(1));
        }
        this.values = function() {
          var values = [];
          if (this.value.length > 0) {
            values = values.concat(this.value);
          }
          for (var key in this.dictionary) {
            if (this.dictionary.hasOwnProperty(key)) {
              values = values.concat(this.dictionary[key].values());
            }
          }
          return values;
        }
      }
      function Task(name, description, html) {
        this.name = name;
        // text-overflow = ellipsis
        this.description = description;
        this.html = html;
      }
      function jumpTo(taskName) {
        toggleSource(taskName, 'open');
        window.location = "#"+taskName;
      }
      function toggleSource(taskName, openclose) {
        var taskSourceLink = document.getElementById(taskName+"-source-link");

        if (openclose == 'close' || taskSourceLink.nextElementSibling.style.display == 'block' && typeof(openclose) == 'undefined') {
          taskSourceLink.nextElementSibling.style.display = 'none';
          taskSourceLink.innerHTML = '[+] Show Source'
        } else if (openclose == 'open' || typeof(openclose) == 'undefined'){
          taskSourceLink.nextElementSibling.style.display = 'block';
          taskSourceLink.innerHTML = '[-] Hide Source';
        };
      }
      function searchTasks(key) {
        document.getElementById('task-search-results').innerHTML = "";
        document.getElementById('task-search-results').style.display = 'none';
        var titleResults = [];
        var descriptionResults = [];
        titleResults = titleResults.concat(taskNameTrie.searchTree(key).values());
        var moreResults = taskNameSplitTrie.searchTree(key).values();
        // dedupe
        for (var i=0; i<moreResults.length; i++) {
          if (titleResults.indexOf(moreResults[i]) == -1) {
            titleResults.push(moreResults[i]);
          }
        }
        if (titleResults.length > 0) {
          document.getElementById('task-search-results').style.display = 'block';
        }
        for (var i=0; i<titleResults.length; i++) {
          var name = titleResults[i].name;
          var nameKeySplit = name.split(key);
          name = nameKeySplit.join("<span class='highlight-search-term'>"+key+"</span>");
          document.getElementById('task-search-results').innerHTML = document.getElementById('task-search-results').innerHTML + titleResults[i].html;
          var result = document.getElementById(titleResults[i].name).getElementsByClassName("search-result")[0];
          result.style.display='block';
          if (i % 2 == 0) {
            result.className = "search-result even"
          } else {
            result.className = "search-result odd"
          }
          document.getElementById(titleResults[i].name).getElementsByClassName("task-info")[0].style.display='none';
          document.getElementById(titleResults[i].name+"-search-result-title").innerHTML=name;
        }
        // slow; replace later
        var pattern = new RegExp(key, "i");
        for (var i=0; i<tasks.length; i++) {
          if (pattern.test(tasks[i].description)) {
            // dedupe
            if (titleResults.indexOf(tasks[i]) == -1) {
              descriptionResults.push(tasks[i]);
            }
          }
        }
        if (descriptionResults.length > 0) {
          document.getElementById('task-search-results').style.display = 'block';
        }
        for (var i=0; i<descriptionResults.length; i++) {
          var description = descriptionResults[i].description;
          var descriptionKeySplit = description.split(key);
          description = descriptionKeySplit.join("<span class='highlight-search-term'>"+key+"</span>");
          var titleCaseDescriptionSplit = description.split(key.charAt(0).toUpperCase() + key.substr(1));
          description = titleCaseDescriptionSplit.join("<span class='highlight-search-term'>"+key.charAt(0).toUpperCase() + key.substr(1)+"</span>");
          document.getElementById('task-search-results').innerHTML = document.getElementById('task-search-results').innerHTML + descriptionResults[i].html;
          var result = document.getElementById(descriptionResults[i].name).getElementsByClassName("search-result")[0];
          result.style.display='block';
          if ((titleResults.length + i) % 2 == 0) {
            result.className = "search-result even"
          } else {
            result.className = "search-result odd"
          }
          document.getElementById(descriptionResults[i].name).getElementsByClassName("task-info")[0].style.display='none';
          document.getElementById(descriptionResults[i].name+"-search-result-description").innerHTML = description;
        }
      }
      var taskNameTrie = new Trie();
      var taskNameSplitTrie = new Trie();
      var tasks = [];
      window.onload = function() {
        <% home.each_task do |task| %>
        var task = new Task("<%= task.name %>", "<%= task.desc %>", <%= render_task(task).to_json %>);
        tasks.push(task);
        taskNameTrie.insert(task.name.toLowerCase(), task);
        var taskNameSplit = task.name.split('_');
        for (var i = 1; i< taskNameSplit.length; i++) {
          taskNameSplitTrie.insert(taskNameSplit[i].toLowerCase(), task);
        }
        <% end %>
        searchTasks('');
      }
    </script>
  </body>
</html>
